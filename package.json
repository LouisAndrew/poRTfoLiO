{
    "name": "gatsby-starter-typescript-deluxe",
    "description": "A Gatsby starter with TypeScript, Storybook, Styled Components, Jest, and more.",
    "version": "0.0.3",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/gojutin/gatsby-starter-typescript-deluxe"
    },
    "bugs": {
        "url": "https://github.com/gojutin/gatsby-starter-typescript-deluxe/issues"
    },
    "scripts": {
        "develop": "gatsby develop",
        "dev": "npm run develop",
        "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
        "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
        "lint:watch": "esw -w . --ext ts --ext tsx --ext js --ext jsx",
        "lint:fix": "npm run lint --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "build": "gatsby build",
        "update": "ncu -u",
        "verify": "gatsby build && gatsby clean && echo -e '${RED}Verified. Page is ready to be pushed.${NC}'"
    },
    "dependencies": {
        "@types/rebass": "^4.0.7",
        "framer-motion": "^1.9.1",
        "gatsby": "^2.19.32",
        "gatsby-image": "^2.2.43",
        "gatsby-plugin-manifest": "^2.2.44",
        "gatsby-plugin-netlify-cms": "^4.4.0",
        "gatsby-plugin-offline": "^3.0.37",
        "gatsby-plugin-react-axe": "^0.4.0",
        "gatsby-plugin-react-helmet": "^3.1.23",
        "gatsby-plugin-sharp": "^2.4.7",
        "gatsby-plugin-theme-ui": "^0.3.3",
        "gatsby-plugin-typescript": "^2.2.2",
        "gatsby-source-filesystem": "^2.1.50",
        "gatsby-transformer-remark": "^2.10.0",
        "gatsby-transformer-sharp": "^2.3.17",
        "modern-css-reset": "^1.1.0",
        "netlify-cms": "^2.10.68",
        "netlify-cms-app": "^2.13.3",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-helmet": "^5.2.1",
        "rebass": "^4.0.7",
        "theme-ui": "^0.4.0-rc.12",
        "use-media": "^1.4.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.7",
        "@babel/runtime": "^7.10.3",
        "@testing-library/jest-dom": "^5.1.1",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^12.2.2",
        "@types/jest": "^25.1.4",
        "@types/react": "^16.9.23",
        "@types/react-dom": "^16.9.5",
        "@types/react-helmet": "^5.0.15",
        "@types/react-test-renderer": "^16.9.3",
        "@typescript-eslint/eslint-plugin": "^2.22.0",
        "@typescript-eslint/eslint-plugin-tslint": "^2.22.0",
        "@typescript-eslint/parser": "^2.22.0",
        "babel-jest": "^25.1.0",
        "babel-loader": "^8.0.6",
        "babel-preset-gatsby": "^0.2.31",
        "babel-preset-react-app": "^9.1.2",
        "eslint": "^6.8.0",
        "eslint-config-gojutin": "0.0.16",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-immutable": "^1.0.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^2.5.0",
        "eslint-watch": "^6.0.1",
        "gatsby-plugin-root-import": "^2.0.5",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^25.1.0",
        "jest-styled-components": "^7.0.0",
        "npm-check-updates": "^4.0.4",
        "prettier": "^1.19.1",
        "react-test-renderer": "^17.0.1",
        "ts-jest": "^25.2.1",
        "tslint": "^6.0.0",
        "tslint-immutable": "^6.0.1",
        "tslint-react": "^4.2.0",
        "typescript": "^3.8.3"
    },
    "resolutions": {
        "**/@babel/runtime": "7.5.0"
    }
}
